extends layout

block content
  .main.container
    .row
      .col-md-6.col-md-offset-3
        h4.display-1.m-b-1 #{title}
  
  .user-list
    table(border="1")
      thead
        th ID 
        th username
      tbody
        each user in users
          tr
            td #{user.getId()}
            td #{user.getName()}
